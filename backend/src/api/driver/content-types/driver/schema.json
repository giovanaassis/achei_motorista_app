{
  "kind": "collectionType",
  "collectionName": "drivers",
  "info": {
    "singularName": "driver",
    "pluralName": "drivers",
    "displayName": "Driver"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "gender": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "homem",
        "mulher",
        "outro"
      ]
    },
    "phone_number": {
      "type": "biginteger",
      "required": false,
      "min": "10",
      "max": "11"
    },
    "vehicle_type": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "carro",
        "moto"
      ]
    },
    "vehicle_seats": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "vehicle_type"
            },
            "carro"
          ]
        }
      },
      "min": 2
    },
    "driver_availability": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::calendar-day.calendar-day",
      "mappedBy": "drivers"
    },
    "driver_socials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::driver-social.driver-social",
      "mappedBy": "driver"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "driver"
    },
    "state": {
      "type": "string",
      "minLength": 3
    },
    "city": {
      "type": "string",
      "minLength": 3
    }
  }
}
